/* pages/register/register.wxss */
@import"/pages/login/login.wxss";
.phoneSno{
  position: relative;
  margin: 0 auto;
  margin-top: 5px;
  width: 98%;
  height: 410px;
   z-index: 1; 
}
.log{
  position: relative;
  width: 100%;
  height: 360px;
  overflow: hidden;
  background: rgb(245, 243, 247);
  z-index: 1; 
}
.inp-text{
  width: 100%;
  height: 100rpx;
  border-radius:10px; 
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-items: center;
  padding-left:10px;
  position: relative;
  margin-top: 5px;
  margin-bottom: 5px;
  border-bottom:1px solid #ccc; 
}
.sendCode{
  width: 80%;
  height: 35px;
  line-height: 35px;
  text-align: center;
  color: rgb(207, 92, 9);
  background: rgb(204, 204, 204);
 }